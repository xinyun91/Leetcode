public class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode a=head;
        ListNode b=head;
        while(n-->0) a=a.next;
        if(a==null) return head.next;
        else{
            while(a.next!=null){a=a.next; b=b.next;}
            b.next=b.next.next;
        }
        return head;
    }
}
